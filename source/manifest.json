{
	"name": "Focas - Flow-optimising Auto Scheduler",
	"version": "0.1.0",
	"description": "Automatically schedule the day, with the aim of optimising the amount of flow you can reach.",
	"homepage_url": "https://github.com/rollersteaam/flow-optimised-calendar-auto-scheduler",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"128": "icon.png"
	},
	"permissions": ["storage", "identity"],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Focas",
		"default_popup": "app/dist/index.html"
	},
	"web_accessible_resources": ["app/dist/img/*.png"],
	"browser_specific_settings": {
		"gecko": {
			"id": "rollersteaam@gmail.com",
			"strict_min_version": "67.0"
		}
	}
}
